<%= render 'users/list', users: @users %>

<% @books.each do |book| %>

  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Opinion</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= book.title %></td>
        <td><%= book.body %></td>
      </tr>
    </tbody>
  </table>

<% end %>