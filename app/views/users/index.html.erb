<div class="container px-5 px-sm-0">

<%= render 'list', users: @users %>

<h2>Users</h2>

<% @users.each do |user| %>

<div class="col-md-8 offset-md-1">
  <table>
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td></td>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', user_path(user.id) %></td>
      </tr>
    </tbody>
  </table>
</div>

<% end %>

</div>

